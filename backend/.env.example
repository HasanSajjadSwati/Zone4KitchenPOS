# Backend Server Configuration
PORT=3001

# Environment Configuration
# Set NODE_ENV to 'production' for production settings
# Set NODE_ENV to 'development' for development settings
# Default: development
NODE_ENV=development

# Logging Configuration
# Set DEBUG to 'true' to enable console logging, 'false' to disable
DEBUG=true

# Database Configuration
DATABASE_URL=postgres://user:password@localhost:5432/pos
# Enable SSL for managed Postgres providers if required
DB_SSL=false
DB_SSL_REJECT_UNAUTHORIZED=true
